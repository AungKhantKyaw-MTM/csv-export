<div id="<%= dom_id post %>">
  <p>
    <strong>Tile:</strong>
    <%= post.tile %>
  </p>

  <p>
    <strong>Body:</strong>
    <%= post.body %>
  </p>

  <p>
    <strong>Author:</strong>
    <%= post.author %>
  </p>

  <p>
    <strong>Likes:</strong>
    <%= post.likes %>
  </p>

  <p>
    <strong>Comments:</strong>
    <%= post.comments %>
  </p>

</div>
